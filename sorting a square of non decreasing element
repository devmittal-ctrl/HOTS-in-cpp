#include<iostream>
#include<vector>
using namespace std;
void logic(vector <int> &v){
   for(int i=0;i<v.size();i++){
      v[i]*=v[i]; //squaring all the elements without sorting
   }
   int j=1;
   while(j<v.size()){ // sorting them
   for(int i=0;i<v.size();i++){
      if(v[i]>v[i+1]) swap(v[i],v[i+1]);
   }
   j++;
}
   
}
int main(){
  // sorting thr squares of the non decreasing arry 
  int n;
  cout<<"enter the number of the elements you want to enter in the array : ";
  cin>>n;
  vector <int> v;
  for(int i=0;i<n;i++){
   int ele;cin>>ele;
   v.push_back(ele);
   if(v[i]<0) v[i]*=-1; // making all the negative numbers into positive
  }
  logic(v);
  for(int i=0;i<n;i++){
   cout<<v[i]<<" ";
  }
  
   return 0;
}
